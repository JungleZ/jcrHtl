<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Find A Service Store Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <normalConfig
                jcr:primaryType="nt:unstructured"
                jcr:title="Normal Config"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mapType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Map Type"
                                name="./mapType">
                                <items jcr:primaryType="nt:unstructured">
                                    <normal
                                        jcr:primaryType="nt:unstructured"
                                        text="Normal"
                                        value="normal"/>
                                    <fixed
                                        jcr:primaryType="nt:unstructured"
                                        text="Fixed"
                                        value="fixed"/>
                                    <withDistance
                                        jcr:primaryType="nt:unstructured"
                                        text="With Distance"
                                        value="withDistance"/>
                                </items>
                            </mapType>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./title"/>
                            <defaultProvinceText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Default Province Text"
                                name="./defaultProvinceText"/>
                            <defaultCityText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Default City Text"
                                name="./defaultCityText"/>
                            <submitButtonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Submit Button Text"
                                name="./submitButtonText"/>
                            <popupSearchDefaultText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Popup Search Default Text"
                                name="./popupSearchDefaultText"/>
                            <popupButton1Text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Popup Button 1 Text"
                                name="./popupButton1Text"/>
                            <popupButton2Text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Popup Button 2 Text"
                                name="./popupButton2Text"/>
                            <provinceApiUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="PROVINCE API URL"
                                name="./provinceApiUrl"/>
                            <cityApiUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="CITY API URL"
                                name="./cityApiUrl"/>
                            <normalTypeApiUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="NORMAL TYPE API URL"
                                name="./normalTypeApiUrl"/>
                            <searchDefaultText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Search Default Text"
                                name="./searchDefaultText"/>
                            <defaultLocation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Default Location"
                                name="./defaultLocation"/>
                            <annotation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldDescription="Put multiple annotations inside &lt;li> html tag"
                                fieldLabel="Annotation"
                                name="./annotation"
                                resize="vertical"/>
                        </items>
                    </column>
                </items>
            </normalConfig>
            <fixedType
                jcr:primaryType="nt:unstructured"
                jcr:title="Fixed Type"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <latitude
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Latitude"
                                name="./latitude"/>
                            <longitude
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Longitude"
                                name="./longitude"/>
                            <fixedTypeApiUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="FIXED TYPE API URL"
                                name="./fixedTypeApiUrl"/>
                        </items>
                    </column>
                </items>
            </fixedType>
            <withDistanceType
                jcr:primaryType="nt:unstructured"
                jcr:title="With Distance Type"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <distances
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Distances">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested="NODE_STORE"
                                    name="./distances">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <title
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Title"
                                                    name="./title"/>
                                                <value
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Value"
                                                    name="./value"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </distances>
                            <withDistanceTypeApiUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="WITH DISTANCE TYPE API URL"
                                name="./withDistanceTypeApiUrl"/>
                        </items>
                    </column>
                </items>
            </withDistanceType>
            <messages
                jcr:primaryType="nt:unstructured"
                jcr:title="messages"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <componentErrorMessages
                                cq:showOnCreate="{Boolean}true"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Component Error Messages"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <provinceErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Province Error Message"
                                        name="./provinceErrorMessage"/>
                                    <cityErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="City Error Message"
                                        name="./cityErrorMessage"/>
                                    <inputErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Input Error Message"
                                        name="./inputErrorMessage"/>
                                    <noResultMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="No Result Message"
                                        name="./noResultMessage"/>
                                    <noServiceMapLocationMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="No Service Map Location Message"
                                        name="./noServiceMapLocationMessage"/>
                                </items>
                            </componentErrorMessages>
                            <googleMapErrorMessages
                                cq:showOnCreate="{Boolean}true"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Google Map Error Messages"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <errorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Error Message"
                                        name="./errorMessage"/>
                                    <zeroResultMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Zero Result Message"
                                        name="./zeroResultMessage"/>
                                    <invalidRequestMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Invalid Request Message"
                                        name="./invalidRequestMessage"/>
                                    <overQueryLimitMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Over Query Limit Message"
                                        name="./overQueryLimitMessage"/>
                                    <requestDeniedMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Request Denied Message"
                                        name="./requestDeniedMessage"/>
                                    <unknownErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Unknown Error Message"
                                        name="./unknownErrorMessage"/>
                                </items>
                            </googleMapErrorMessages>
                        </items>
                    </column>
                </items>
            </messages>
        </items>
    </content>
</jcr:root>
