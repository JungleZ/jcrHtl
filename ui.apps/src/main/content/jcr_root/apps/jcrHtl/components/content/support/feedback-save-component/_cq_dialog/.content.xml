<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Feedback Save Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <contentTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <yesNoDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Yes/No Description"
                                fieldLabel="Yes/No Description"
                                name="./yesNoDescription"/>
                            <yesOptionText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Yes Option Text"
                                fieldLabel="Yes Option Text"
                                name="./yesOptionText"/>
                            <noOptionText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="No Option Text"
                                fieldLabel="No Option Text"
                                name="./noOptionText"/>
                            <emailDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Email Description"
                                fieldLabel="Email Description"
                                name="./emailDescription"/>
                            <emailOptionText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Email Option Text"
                                fieldLabel="Email Option Text"
                                name="./emailOptionText"/>
                            <commentDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Comment Description"
                                fieldLabel="Comment Description"
                                name="./commentDescription"/>
                            <submitButtonText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Submit Button Text"
                                fieldLabel="Submit Button Text"
                                name="./submitButtonText"/>
                            <successMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Success Message"
                                fieldLabel="Success Message"
                                name="./successMessage"/>
                            <errorMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Error Message"
                                fieldLabel="Error Message"
                                name="./errorMessage"/>
                            <faqIDErrorMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="FAQ ID Error Message"
                                name="./faqIDErrorMessage"/>
                            <faqCommentsLenghtErrorMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="FAQ Comment Lenght Error Message"
                                name="./faqCommentsLenghtErrorMessage"/>
                            <faqCommentsEmptyErrorMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="FAQ Comment Empty Error Message"
                                name="./faqCommentsEmptyErrorMessage"/>
                            <contactUsURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Contact Us URL"
                                fieldLabel="Contact Us URL"
                                name="./contactUsURL"
                                rootPath="/content/huawei-cbg-site"/>
                            <emailIconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Email Icon Image"
                                name="./emailIconImage"
                                rootPath="/content/dam"/>
                            <yesIconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Yes Icon Image"
                                name="./yesIconImage"
                                rootPath="/content/dam"/>
                            <noIconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="No Icon Image"
                                name="./noIconImage"
                                rootPath="/content/dam"/>
                            <getFAQInfoURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="API URL to get FAQ info"
                                fieldLabel="Get FAQ Info URL"
                                name="./getFAQInfoURL"/>
                            <sendYesCommentURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="API URL to send comment when user click YES button"
                                fieldLabel="Send Yes comment URL"
                                name="./sendYesCommentURL"/>
                            <sendNoCommentURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="API URL to send comment when user click NO button"
                                fieldLabel="Send No comment URL"
                                name="./sendNoCommentURL"/>
                        </items>
                    </column>
                </items>
            </contentTab>
        </items>
    </content>
</jcr:root>
